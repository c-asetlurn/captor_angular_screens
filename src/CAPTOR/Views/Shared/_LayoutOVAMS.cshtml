                            <li class="my-reports"><a asp-controller="Enterprise" asp-action="Reports"><span> My Reports</span></a></li>
                            <li class="divider"></li>
                            <li class="logout"><a asp-controller="Login" asp-action="Login"><span> Log Out</span></a></li>
                        </ul>
                    </li>
                    <li class="right-sidebar-toggle">
                        <a href="javascript:;" data-click="right-sidebar-toggled">
                            <span></span>
                        </a>
                    </li>
                </ul>
            </div>
        </div>

        <div id="sidebar" class="sidebar">
            <ul class="nav">
                <li class="nav-logo">
                    <a asp-controller="Dashboard" asp-action="Dashboard" class="logo"><img src="~/Content/img/captorlogo.png" class="logo" alt="" /> </a>
                </li>
                <li class="mini-logo">
                    <a asp-controller="Dashboard" asp-action="Dashboard" class="logo"><img src="~/Content/img/mini-logo.png" class="logo" alt="" /> </a>
                </li>
                <li class="divider has-minify-btn">
