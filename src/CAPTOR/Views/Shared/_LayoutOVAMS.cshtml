                <li class="sidebar-menu-item search-offender">
                    <a asp-controller="Enterprise" asp-action="OffenderSearch">
                        <span class="icon"></span>
                        <span class="icon2"></span>
                        <span>Individual Reentrant Search</span>
                    </a>
                </li>
                <li class="sidebar-menu-item ovams-menu has-sub">
                    <a href="javascript:;">
                        <b class="caret pull-right"></b>
                        <span class="icon"></span>
                        <span class="icon2"></span>
                        <span>OVAMS</span>
                    </a>
                    <ul class="sub-menu">
                        <li class="sidebar-menu-item">
                            <a asp-controller="OvamsController" asp-action="newclient" href="newclient">New Application</a>
                        </li>
                        <li class="sidebar-menu-item">
                            <a asp-controller="OvamsController" asp-action="searchClient" href="searchClient">Search Application</a>
                        </li>
                    </ul>
                </li>
            </ul>
        </div>
        <div class="sidebar-bg"></div>

        <div id="content" class="content">

            @RenderBody()

            <div id="footer" class="footer">
                <span class="pull-right">
                    <a href="javascript:;" class="btn-scroll-to-top" data-click="scroll-top">
                        <i class="fa ion-chevron-up"></i> <span class="hidden-xs">Back to Top</span>
                    </a>
                </span>
                &copy; 2016 <b>CAPTOR - Commonwealth of Pennsylvania</b> All Rights Reserved
            </div>
        </div>
        <ng-include src="'../common/RightsideBar/clientProfileBanner.html'"></ng-include>
        <div class="sidebar-bg-right"></div>
        
