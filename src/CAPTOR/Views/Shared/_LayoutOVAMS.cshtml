    {
        <script src="~/Content/js/bundle-apps-top.min.js"></script>
        <script src="~/lib/angular/angular.min.js"></script>
    }
    else
    {
        <script src="~/Content/js/apps-validation.js"></script>
        <script src="~/Content/js/apps-dropdown.js"></script>

        //libs strat
        <script src="~/lib/angular/angular.js"></script>
        <script src="~/lib/angular-route/angular-route.min.js"></script>
        <script src="http://angular-ui.github.io/ui-router/release/angular-ui-router.min.js"></script>
        <script src="https://code.angularjs.org/1.4.9/angular-animate.js"></script>
        @*<script src="~/lib/angular-bootstrap/ui-bootstrap.js"></script>*@
        @*<script src="~/lib/angular-bootstrap/ui-bootstrap-tpls.js"></script>*@
        <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.5.8/angular-sanitize.js"></script>
        <script src="//angular-ui.github.io/bootstrap/ui-bootstrap-tpls-2.4.0.js"></script>
        //libs ends
        <script src="~/app.module.js"></script>
        //controllers start
        <script src="~/OVAMS/ovamsctrl.js"></script>
        <script src="~/common/RightsideBar/rightsideCtrl.js"></script>
        //controllers end
        
        <script src="~/common/RightsideBar/profileviewDirective.js"></script>
        <script src="~/common/RightsideBar/offenderProfileviewDirective.js"></script>
        <script src="~/common/Directives/infiniteScrolling.js"></script>
        <script src="~/common/Directives/flip/flip.js"></script>
        

    }
    <!-- Need to Replace this code in Future, Just Done for now to help Case Notes Deployment-->
    <script>
        var RootURI = "@Url.Content("~")";
    </script>
