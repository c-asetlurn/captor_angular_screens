<head>
    <meta charset="utf-8" />
    <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" name="viewport" />
    <title>@ViewBag.Title - CAPTOR</title>
    <meta content="" name="description" />
    <meta content="" name="author" />
    <meta http-equiv="X-UA-Compatible" content="IE=Edge,chrome=1" />
    @{var env = Environment.GetEnvironmentVariable("ENV"); }
    <link rel='shortcut icon' type='image/x-icon' href="~/Content/img/favicon.ico" />
    <!--Plugin Stylesheets-->
    @if (!string.IsNullOrEmpty(env) && (env.ToLower().Trim() == "dev" || env.ToLower().Trim() == "sit" || env.ToLower().Trim() == "uat" || env.ToLower().Trim() == "prod"))
    {
        <link href="~/Content/plugins/plugin-bundle-top.min.css" rel="stylesheet" />
    }
    else
    {
        <link href="~/Content/plugins/jquery-ui/themes/base/minified/jquery-ui.min.css" rel="stylesheet" />
        <link href="~/Content/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
        <link href="~/Content/plugins/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
        <link href="~/Content/plugins/ionicons/css/ionicons.min.css" rel="stylesheet" />
        <link href="~/Content/plugins/icomoon/css/icomoon.min.css" rel="stylesheet" />
        <link href="~/Content/plugins/jquery-tag-it/css/jquery.tagit.min.css" rel="stylesheet" />
        <link href="~/Content/plugins/slick/slick/slick.min.css" rel="stylesheet" />
        <link href="~/Content/plugins/slick/slick/slick-theme.min.css" rel="stylesheet" />
        <link href="~/Content/plugins/prettyPhoto/css/prettyPhoto.min.css" rel="stylesheet" />
        <link href="~/Content/css/flip.css" rel="stylesheet" />
    }
    <!--Custom Stylesheets-->

    @if (!string.IsNullOrEmpty(env) && (env.ToLower().Trim() == "dev" || env.ToLower().Trim() == "sit" || env.ToLower().Trim() == "uat" || env.ToLower().Trim() == "prod"))
    {
        <link href="~/Content/css/bundle-apps.min.css" rel="stylesheet" />
    }
    else
    {
        <link href="~/Content/css/animate.min.css" rel="stylesheet" />
        <link href="~/Content/css/main.min.css" rel="stylesheet" />
        <link href="~/Content/css/tooltip.min.css" rel="stylesheet" />
    }
    <!--Plugin Scripts-->
    @if (!string.IsNullOrEmpty(env) && (env.ToLower().Trim() == "dev" || env.ToLower().Trim() == "sit" || env.ToLower().Trim() == "uat" || env.ToLower().Trim() == "prod"))
    {
        <script src="~/Content/plugins/bundle-plugin-top.min.js"></script>
    }
    else
    {
        <script src="~/Content/plugins/jquery/jquery-2.1.4.min.js"></script>
        <script src="~/Content/plugins/jquery-ui/jquery-ui.min.js"></script>
        <script src="~/Content/plugins/bootstrap/js/bootstrap.min.js"></script>
    }

