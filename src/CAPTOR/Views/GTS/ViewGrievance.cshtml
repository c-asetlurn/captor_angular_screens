    function findinvolvedkeyword(passdescription, keyword, description) {
        for (counter_description = 0; counter_description < description.length; counter_description++) {
            if (description[counter_description] === passdescription) {
                found_keyword = keyword[counter_description];

                if (found_keyword == "STA") {
                    $('.offender-search').hide();
                    $('.staff-search').show();
                    $('.other-search').hide();
                    $('.involved-description').hide();
                }
                else if (found_keyword == "REE") {
                    $('.offender-search').show();
                    $('.staff-search').hide();
                    $('.other-search').hide();
                    $('.involved-description').hide();

                }
                else if (found_keyword == "OTH") {
                    $('.offender-search').hide();
                    $('.staff-search').hide();
                    $('.other-search').show();
                    $('.involved-description').show();
                }

            }
        }
    }

